<%- include header %>
<form method="post">
	<div class="loginAccountTitle">
		用户名：
	</div>
	<div class="aaa">
		<input type="text" name="name" size="36"  class="loginAccountEdit" width="200px" onmouseover="this.style.borderColor='black';this.style.backgroundColor='plum'" onmouseout="this.style.borderColor='black';this.style.backgroundColor='#ffffff'" />
	</div>
	<div class="loginAccountTitle">
		密码：
	</div>
	<div class="aaa">
		<input type="password" name="password"  class="loginAccountEdit" size="36" width="200px" onmouseover="this.style.borderColor='black';this.style.backgroundColor='plum'" onmouseout="this.style.borderColor='black';this.style.backgroundColor='#ffffff'"  />
	</div>
	<div>
		<input class="submitBtn" type="submit" value="登录">
	</div>
</form>
<%- include footer %>